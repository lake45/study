<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.system.domain.mapper.BannerMapper">


  <select id="getBannerMessage" resultType= "com.shop.system.domain.model.BannerModel">
    SELECT
        banner_pic          bannerPic,
        banner_text         bannerText
    FROM
        shop_banner
    WHERE
        is_delete = 0
  </select>

</mapper>
